    "import math \n",
    "def ktcp(a):\n",
    "    can =math.isqrt(a)#nếu muốn sáng tạo thêm thì dùng hàm round\n",
    "    if a==can*can:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "if __name__=='__main__':\n",
    "    a=int(input())\n",
    "    if ktcp(a):\n",
    "        print('Yes')\n",
    "    else:\n",
